{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "singularName": "package",
    "pluralName": "packages",
    "displayName": "Package"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "package_name": {
      "type": "string"
    },
    "subname": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image_url": {
      "type": "string"
    },
    "price": {
      "type": "string"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "mappedBy": "package_id"
    },
    "product": {
      "type": "string"
    }
  }
}
