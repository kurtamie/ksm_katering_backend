{
  "kind": "collectionType",
  "collectionName": "staffs",
  "info": {
    "singularName": "staff",
    "pluralName": "staffs",
    "displayName": "Staff"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "staff"
    },
    "department": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "position": {
      "type": "string"
    },
    "ktp_no": {
      "type": "string"
    },
    "staff_status": {
      "type": "enumeration",
      "default": "Active",
      "enum": [
        "Active",
        "Non_active",
        "Blocked"
      ]
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "staff_id"
    },
    "orders_driver": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "staff_driver_id"
    },
    "customers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::customer.customer",
      "mappedBy": "staff_id"
    }
  }
}
